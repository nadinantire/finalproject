


<div class="row">
  <div class="col-sm-6">
    <h1>Garelleys</h1>
  </div>

  <div class="col-sm-6 text-right">
  <%= link_to new_garelley_path, class: 'btn btn-primary' do %>
    Add New Garelley
  <% end %>
  </div>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
    
        <th>Name</th>
    

            <th>Image</th>
            
      </tr>
    </thead>

    <tbody>
      <% @garelleys.each do |garelley| %>
        <%= content_tag :tr, id: dom_id(garelley), class: dom_class(garelley) do %>
          
            <td><%= link_to garelley.name, garelley %></td>
          

                      <td><%= garelley.image %></td>
          
          
        <% end %>
      <% end %>
    </tbody>
  </table>
</div>
