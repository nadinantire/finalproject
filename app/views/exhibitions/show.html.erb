<div class="page-header">
  <%= link_to exhibitions_path, class: 'btn btn-default' do %>
    All Exhibitions
  <% end %>
  <%= link_to edit_exhibition_path(@exhibition), class: 'btn btn-primary' do %>
    Edit
  <% end %>
  <h1>Show exhibition</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @exhibition.name %></dd>

  <dt>Start time:</dt>
  <dd><%= @exhibition.start_time %></dd>

  <dt>End time:</dt>
  <dd><%= @exhibition.end_time %></dd>

  <dt>Image:</dt>
  <dd><%= @exhibition.image %></dd>

  <dt>Date:</dt>
  <dd><%= @exhibition.date %></dd>

</dl>
